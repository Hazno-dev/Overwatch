            MoiraPhase = false;
            break;
        case Hero.Roadhog:
            SetAbility1Enabled(EventPlayer(), true);
            DisallowButton(EventPlayer(), Button.PrimaryFire);
            MoiraPhase = false;
            break;
        case Hero.Mercy:
            SetAbility1Enabled(EventPlayer(), true);
            DisallowButton(EventPlayer(), Button.PrimaryFire);
            MoiraPhase = false;
            break;
        case Hero.Ashe:
            SetAbility1Enabled(EventPlayer(), true);
            DisallowButton(EventPlayer(), Button.PrimaryFire);
            MoiraPhase = false;
            break;
        case Hero.Ana:
            SetAbility1Enabled(EventPlayer(), true);
            DisallowButton(EventPlayer(), Button.PrimaryFire);
            MoiraPhase = false;
            break;
        case Hero.Junkrat:
