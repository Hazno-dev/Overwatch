            break;
        case Hero.Widowmaker:
            SetAbility1Enabled(EventPlayer(), false);
            MoiraPhase = false;
            break;
        case Hero.Genji:
            SetAbility1Enabled(EventPlayer(), false);
            DisallowButton(EventPlayer(), Button.PrimaryFire);
