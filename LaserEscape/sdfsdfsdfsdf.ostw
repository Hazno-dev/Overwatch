}

rule: "Even Round"
if (Round % 2 == 0)
{
    Direc2();
    # RoundSpeed1()\nwait(1.25, Wait.ABORT_WHEN_FALSE)
    DeathDirec2();
    Direct2 = true;
    Direct1 = false;
}

rule: "Odd Round"
